{
  "name": "uilab",
  "version": "0.0.0",
  "description": "Everything related to UI",
  "license": "MIT",
  "author": "altanbagana@ailab.mn",
  "private": true,
  "scripts": {
    "dev:docs": "pnpm --prefix apps/docs run dev",
    "start:docs": "pnpm --prefix apps/docs run start",
    "build:docs": "pnpm --prefix apps/docs run build",
    "build:components-registry": "pnpm --prefix packages/components run registry:build",
    "build:components": "pnpm --prefix packages/components run build",
    "format": "prettier --cache --write '{apps,packages}/**/*.{js,jsx,ts,tsx,css,md,mdx,json}'"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14"
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "pnpm": "10.18",
    "node": ">=22"
  }
}
