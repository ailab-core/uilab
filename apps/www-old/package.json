{
  "name": "www-old",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "serve dist/client",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "registry:build": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --log-level silent --write \"registry/**/*.{ts,tsx,json,mdx}\" --cache",
    "registry:capture": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/capture-registry.mts",
    "validate:registries": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/validate-registries.mts",
    "types:check": "fumadocs-mdx && tsc --noEmit",
    "postinstall": "fumadocs-mdx"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@orama/orama": "^3.1.18",
    "@tanstack/react-router": "^1.142.13",
    "@tanstack/react-router-devtools": "^1.142.13",
    "@tanstack/react-start": "^1.142.13",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/start-static-server-functions": "^1.142.13",
    "class-variance-authority": "^0.7.1",
    "fumadocs-core": "16.3.2",
    "fumadocs-mdx": "14.2.2",
    "fumadocs-ui": "16.3.2",
    "lucide-react": "^0.562.0",
    "lucide-static": "^0.562.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-number-format": "^5.4.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "uilab-core": "^0.2.22",
    "vite": "^7.3.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "serve": "^14.2.5",
    "shadcn": "3.6.2",
    "shiki": "^3.20.0",
    "tailwindcss": "^4.1.18",
    "ts-morph": "^27.0.2",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.3"
  }
}
