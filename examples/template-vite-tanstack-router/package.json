{
  "name": "template-vite-tanstack-router",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-devtools": "^0.8.1",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-router": "^1.136.1",
    "@tanstack/react-router-devtools": "^1.136.1",
    "@tanstack/router-plugin": "^1.136.1",
    "axios": "^1.13.2",
    "i18next": "^25.6.2",
    "lucide-react": "^0.553.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.3",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "uilab-core": "^0.1.9"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.9",
    "web-vitals": "^5.1.0"
  },
  "prettier": {
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ],
    "arrowParens": "always",
    "printWidth": 80,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "endOfLine": "lf",
    "objectWrap": "preserve",
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "importOrder": [
      "^(react/(.*)$)|^(react$)",
      "^(next/(.*)$)|^(next$)",
      "<THIRD_PARTY_MODULES>",
      "^types$",
      "^(@/assets/(.*)$)|^(@/assets$)",
      "^(@/components/(.*)$)|^(@/components$)",
      "^(@/components/ui/(.*)$)|^(@/components/ui$)",
      "^(@/components/blocks(.*)$)|^(@/components/blocks$)",
      "^(@/data/(.*)$)|^(@/data$)",
      "^(@/hooks/(.*)$)|^(@/hooks$)",
      "^(@/lib/(.*)$)|^(@/lib$)",
      "^(@/providers/(.*)$)|^(@/providers$)",
      "^(@/routes/(.*)$)|^(@/routes$)",
      "^[./]"
    ],
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ]
  }
}
